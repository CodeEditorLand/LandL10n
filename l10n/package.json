{
	"name": "@vscode/l10n",
	"version": "0.0.1",
	"private": false,
	"description": "A helper library to assist in localizing subprocesses spun up by VS Code extensions",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"sideEffects": false,
	"type": "module",
	"main": "./dist/main.js",
	"browser": {
		"./dist/main.js": "./dist/browser.js",
		"./src/node/reader": "./src/browser/reader"
	},
	"types": "dist/main.d.ts",
	"files": [
		"dist/*"
	],
	"scripts": {
		"clean": "rimraf dist && rimraf lib",
		"compile": "npm run clean && tsc --emitDeclarationOnly --outDir lib && node .esbuild.config.js",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "node .esbuild.config.js --watch"
	},
	"devDependencies": {
		"@jest/globals": "29.7.0",
		"@microsoft/api-extractor": "7.43.0",
		"@types/mock-fs": "4.13.4",
		"@types/node": "20",
		"esbuild": "0.20.2",
		"http-proxy-agent": "7.0.2",
		"https-proxy-agent": "7.0.4",
		"jest": "29.7.0",
		"jest-fetch-mock": "3.0.3",
		"mock-fs": "5.2.0",
		"rimraf": "5.0.5",
		"ts-jest": "29.1.2",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
