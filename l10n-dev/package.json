{
	"name": "@vscode/l10n-dev",
	"version": "0.0.1",
	"private": false,
	"description": "Development time npm module to generate strings bundles from TypeScript files",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandL10n#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandL10n/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandL10n.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"bin": {
		"vscode-l10n-dev": "./dist/cli.js"
	},
	"files": [
		"dist/*"
	],
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build-wasm": "npm run build-wasm-typescript && npm run build-wasm-tsx",
		"build-wasm-tsx": "tree-sitter build --wasm ./node_modules/tree-sitter-typescript/tsx --output ./src/ast/tree-sitter-tsx.wasm",
		"build-wasm-typescript": "tree-sitter build --wasm ./node_modules/tree-sitter-typescript/typescript --output ./src/ast/tree-sitter-typescript.wasm",
		"clean": "rimraf dist && rimraf lib",
		"compile": "npm run clean && tsc --emitDeclarationOnly --outDir lib && node .esbuild.config.mjs",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"dependencies": {
		"debug": "4.3.4",
		"deepmerge-json": "1.5.0",
		"glob": "10.3.12",
		"markdown-it": "14.1.0",
		"node-html-markdown": "1.3.0",
		"pseudo-localization": "2.4.0",
		"web-tree-sitter": "0.22.6",
		"xml2js": "0.6.2",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/debug": "4.1.12",
		"@types/markdown-it": "14.1.1",
		"@types/mock-fs": "4.13.4",
		"@types/node": "20.12.10",
		"@types/xml2js": "0.4.14",
		"@types/yargs": "17.0.32",
		"esbuild": "0.20.2",
		"esbuild-copy-files-plugin": "1.2.0",
		"mock-fs": "5.2.0",
		"rimraf": "5.0.5",
		"tree-sitter-cli": "0.22.6",
		"tree-sitter-typescript": "0.21.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
